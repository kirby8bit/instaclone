<%= stylesheet_link_tag('signup') %>

<div id="post">
  <div id="pnav">
    instaclone
    <img class="pbuttons" src="https://31.media.tumblr.com/ecad6f31db9fc8bf8d14f2214dbe5a4b/tumblr_inline_naz3sihqg71sbfysw.png">
    <img class="pbuttons" src="https://media.tumblr.com/3c40921397a12ca56e9185b816e8e280/tumblr_inline_nb0y19IrNy1sbfysw.png">
    <img class="pbuttons" src="https://media.tumblr.com/74cab287d11e5d00036c09c75101a375/tumblr_inline_nb0y13nRs91sbfysw.png">
  </div>
  <div class="form">
    <%= form_for(@user) do |f| %>
    <%= f.label :name, class: "name" %>
    <%= f.text_field :name, class: "name" %>
    <%= f.label :city, class: "city" %>
    <%= f.text_field :city, class: "city" %>
    <%= f.label :country, class: "country" %>
    <%= f.text_field :country, class: "country" %>
    <%= f.submit "Create my account", class: "btn" %>
    <% end %>
  </div>
  <div id="noter">
    <div class="notebox">Welcome !</div>
    <div class="notebox">Join us !</div>
  </div>
</div>
